{
  "name": "autolik.es-frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^18.1.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.0.0",
    "chart.js": "^2.5.0",
    "enzyme": "^2.8.2",
    "immutability-helper": "^2.1.2",
    "immutable": "^3.8.1",
    "jwt-decode": "^2.1.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-breadcrumbs": "^1.5.2",
    "react-chartjs-2": "^2.1.0",
    "react-dom": "^15.5.4",
    "react-ga": "^2.2.0",
    "react-intl": "^2.3.0",
    "react-intl-redux": "^0.5.0",
    "react-ladda": "^5.0.6",
    "react-moment": "^0.2.2",
    "react-redux": "^5.0.4",
    "react-redux-toastr": "^6.2.4",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-stripe-checkout": "^2.2.5",
    "react-test-renderer": "^15.5.4",
    "react-text-mask": "^2.2.0",
    "react-textfield": "0.0.4",
    "reactstrap": "^4.2.0",
    "redux": "^3.6.0",
    "redux-form": "^6.6.1",
    "redux-logger": "^2.8.1",
    "redux-saga": "^0.14.3",
    "reselect": "^2.5.4",
    "sinon": "^2.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build-css": "node-sass src/assets/scss/style.scss -o src/assets/build/",
    "watch-css": "npm run build-css && node-sass src/assets/scss/ -o src/assets/build/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "eslint src/app/**"
  },
  "pre-commit": {
    "run": "eslint",
    "silent": true
  }
}
